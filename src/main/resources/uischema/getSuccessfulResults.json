{
  "connectorName": "salesforce_bulkapi_v2",
  "operationName": "getSuccessfulResults",
  "title": "Get Successful Results",
  "help": "This operation is used to get the successful results of a job.",
  "elements": [
    {
      "type": "attributeGroup",
      "value": {
        "groupName": "General",
        "elements": [
          {
            "type": "attribute",
            "value": {
              "name": "configRef",
              "displayName": "Salesforce Configuration",
              "inputType": "connection",
              "allowedConnectionTypes": [
                "init"
              ],
              "defaultValue": "",
              "required": "true",
              "helpTip": "Connection to be used"
            }
          },
          {
            "type": "attributeGroup",
            "value": {
              "groupName": "Basic",
              "elements": [
                {
                  "type": "attribute",
                  "value": {
                    "name": "jobId",
                    "displayName": "Job ID",
                    "inputType": "stringOrExpression",
                    "defaultValue": "",
                    "required": "true",
                    "helpTip": "The ID of the job that you want to get the successful results for."
                  }
                },
                {
                  "type": "attribute",
                  "value": {
                    "name": "outputType",
                    "displayName": "Output type",
                    "inputType": "comboOrExpression",
                    "comboValues": [
                      "JSON",
                      "CSV"
                    ],
                    "defaultValue": "JSON",
                    "required": "true",
                    "helpTip": "Output Content Type"
                  }
                },
                {
                  "type": "attribute",
                  "value": {
                    "name": "includeResultTo",
                    "displayName": "Add Result To",
                    "inputType": "comboOrExpression",
                    "comboValues": [
                      "FILE",
                      "BODY"
                    ],
                    "defaultValue": "BODY",
                    "required": "true",
                    "helpTip": ""
                  }
                },
                {
                  "type": "attribute",
                  "value": {
                    "name": "filePath",
                    "displayName": "File Path",
                    "inputType": "stringOrExpression",
                    "defaultValue": "",
                    "required": "false",
                    "enableCondition": [
                      {
                        "includeResultTo": "FILE"
                      }
                    ],
                    "helpTip": "The path to the file to write the results to."
                  }
                }
              ]
            }
          }
        ]
      }
    }
  ]
}
